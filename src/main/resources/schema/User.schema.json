{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "User",
  "description": "Object representing a user.",
  "type": "object",
  "properties": {
    "username": { "type": "string" },
    "email": { "type": "string" },
    "password": { "type": "string" },
    "_id": { "type": "string" },
    "networks": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "uniqueItems": true
    }
  },
  "required": [ "username", "email", "password"]
}
